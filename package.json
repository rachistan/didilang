{
  "name": "didilang",
  "displayName": "Didi Language Support",
  "description": "Support for Didi language",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": ["Programming Languages"],
  "contributes": {
    "languages": [{
      "id": "didi",
      "extensions": [".didi"],
      "aliases": ["Didi", "didi"]
    }]
  },
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "npm run compile"
  },
  "devDependencies": {
    "typescript": "^4.4.3",
    "vscode-test": "^1.6.2"
  }
}